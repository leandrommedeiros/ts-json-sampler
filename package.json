  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.19.31",
    "@types/uuid": "^9.0.8",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "ansi-colors": "^4.1.3",
    "cli-progress": "^3.12.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-mock-extended": "^3.0.6",
    "lodash": "^4.17.21",
    "pkg": "^5.8.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "uuid": "^9.0.1",
    "yargs": "^17.7.2"
  }
